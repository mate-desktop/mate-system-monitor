      <_summary>Default graph CPU color</_summary>
    </key>
    <key name="cpu-color11" type="s">
      <default>'#330099'</default>
      <_summary>Default graph CPU color</_summary>
    </key>
    <key name="cpu-color12" type="s">
      <default>'#CC0066'</default>
